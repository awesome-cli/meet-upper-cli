    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/node": "^13.1.0",
    "@types/node-fetch": "^2.5.4",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
