{
  "name": "meet-upper-cli",
  "version": "2.1.1",
  "description": "Search events on Meetup from CLI",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "bin/index.js",
  "scripts": {
    "build": "rimraf bin/* && tsc -p .",
    "prettier": "prettier --write 'lib/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JB1905/meet-upper-cli.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "CLI",
    "api",
    "meetups",
    "events",
    "search"
  ],
  "bugs": {
    "url": "https://github.com/JB1905/meet-upper-cli/issues"
  },
  "homepage": "https://github.com/JB1905/meet-upper-cli#readme",
  "bin": {
    "meet-upper-cli": "bin/index.js",
    "meet-upper": "bin/index.js",
    "meetUPPER": "bin/index.js",
    "MEET": "bin/index.js",
    "MU": "bin/index.js"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "figlet": "^1.2.4",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/node": "^13.1.8",
    "@types/node-fetch": "^2.5.4",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "lib/**/*.ts": [
      "npm run prettier"
    ]
  }
}
